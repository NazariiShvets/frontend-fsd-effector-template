    "effector-view": "^2.0.13",
    "history": "^5.3.0",
    "patronum": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
