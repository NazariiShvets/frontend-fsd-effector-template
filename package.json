{
  "name": "project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "app:start": "yarn gen:api && craco start",
    "gen:api": "node scripts/gen/api.js",
    "test:unit": "craco test --coverage",
    "test:unit:all": "craco test --coverage --watchAll=false",
    "test:unit:watch:all": "craco test --coverage --watchAll",
    "test:sonar": "sonar-scanner",
    "lint:unused-deps": "unimported",
    "lint:js": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "lint:css": "stylelint --fix \"./src/**/*.scss\"",
    "docs:storybook": "start-storybook --no-manager-cache -p 6006",
    "build:app": "craco build",
    "build:storybook": "build-storybook -s public",
    "postinstall": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:js"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ],
    "collectCoverageFrom": [
      "src/**",
      "!src/**/*.stories.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "dependencies": {
    "axios": "^0.27.0",
    "classnames": "^2.3.1",
    "effector": "^22.3.0",
    "effector-http-api": "^2.0.6",
